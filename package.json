        "color-diff": "^1.4.0",
        "comlink": "^4.4.2",
        "copy-to-clipboard": "^3.3.3",
        "core-js": "^3.40.0",
        "dayjs": "^1.11.13",
        "delay": "^6.0.0",
        "element-plus": "^2.9.3",
        "exceljs": "^4.4.0",
