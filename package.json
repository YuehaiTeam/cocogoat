        "vue-tsc": "~2.1.10",
        "webpack-bundle-analyzer": "^4.10.2",
        "worker-loader": "^3.0.8"
    },
    "packageManager": "pnpm@9.15.3",
    "pnpm": {
        "patchedDependencies": {
            "git-repo-info@2.1.1": "patches/git-repo-info@2.1.1.patch",
            "vue-virtual-scroller@2.0.0-alpha.1": "patches/vue-virtual-scroller@2.0.0-alpha.1.patch",
            "vite-plugin-ifdef@1.0.5": "patches/vite-plugin-ifdef@1.0.5.patch",
            "vue-scriptx@0.2.7": "patches/vue-scriptx@0.2.7.patch"
        },
        "peerDependencyRules": {
            "ignoreMissing": [
                "vite",
                "rollup",
                "webpack",
                "@babel/core"
            ]
        },
        "overrides": {
            "flyio>request": "link:.cache/empty"
        }
    }
